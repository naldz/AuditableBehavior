{
    "name": "naldz/propel-auditable-behavior",
    "description": "A propel behavior that will let you check the original field values of a model even after save or delete.",
    "keywords": [ "propel", "behavior" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Reynaldo Castellano III",
            "email": "reynaldocastellano@gmail.com"
        }
    ],
    "require": {
        "propel/propel1": "1.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "classmap": ["src/"]
    },
	"target-dir": "Naldz/Propel/AuditableBehavior",
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}